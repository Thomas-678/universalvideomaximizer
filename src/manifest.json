{
  "name": "Universal Video Maximizer BETA v3",
  "version": "3.0.25",
  "manifest_version": 3,
  "description": "Zoom videos and theater mode on most websites WITHOUT requiring permissions to every site.",
  "homepage_url": "https://chrome.google.com/webstore/support/cdnlfphfngnfhjcnoikfhaomaaflaiie",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "action": {
    "default_title": "Universal Video Maximizer BETA v3"
  },
  "background": {
    "service_worker": "background.js"
  },
  "optional_permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inject.css"
      ],
      "matches": [ "<all_urls>" ],
      "extension_ids": []
    }
  ]
}
