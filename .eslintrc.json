{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-inner-declarations": 0,
        "no-restricted-syntax": 0,
        "no-plusplus": 0,
        "no-bitwise": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "no-continue": 0,
        "no-use-before-define": 0
    }
}
